<%- include('head') %>

<%- include('title', { title: title, breadcrumbs: breadcrumbs }) %>

<section id="main" class="container-fluid" aria-label="main body of content plus related links and features">
  <div class="container-lft">
    <div class="row">
      <h2 class="sr-only">Topic content</h2>
      <div class="col-12 col-lg-8">
        <% for (let card of body.cards) { %>
          <%- include(card.template, { card: card }) %>
        <% } %>
      </div>
      <%- include('rhnav', { rhnav: rhnav, pageTitle: pageTitle, fullUrl: fullUrl }) %>
    </div>
  </div>
</section>

<%- include('foot') %>
<div class="main-and-aside r">
  
  <div class="main content-container" role="main">
    
      <div class="article-teaser">
        There are more than 11,500 bikes at over 750 docking stations across London.<BR>
        
      </div>
    
    
      
        <p>Enter your location in the search box below to find a docking station near you. </p>
    
      
           
      

        <div class="search-filter">
          <div class="heading clearfix">
            <div class="search-icon-black"></div>
            <h3>Search</h3>
          </div>
          @{
            Html.RenderAction("SearchFilter", "Modules", new
            {
              actionUrl = Library.DisambiguationSettings.DisambiguationUrl,
              ajaxEnabled = true,
              currentLocationSubmitUrl = "/maps/cycle-hire",
              additionalFields = new List<string> { "InputGeolocation" },
              buttonText = "Go",
              dataSets = new[] { 
              new[]{ Library.DisambiguationDataSets.PlacesExtra, "/maps/cycle-hire?Input={{input}}&InputGeolocation={{lon}},{{lat}}" },
              new[]{ Library.DisambiguationDataSets.StopPoints, "/maps/cycle-hire?Input={{input}}&InputGeolocation={{lon}},{{lat}}" },
              new[]{ Library.DisambiguationDataSets.BikePoints, "/maps/cycle-hire?Input={{input}}&InputGeolocation={{lon}},{{lat}}" }
              },
              modes = string.Format(
              "{0},{1},{2},{3},{4},{5},{6},{7},{8}",
              Library.MeansOfTransport.Tube,
              Library.MeansOfTransport.DLR,
              Library.MeansOfTransport.Overground,
              Library.MeansOfTransport.TflRail,
              Library.MeansOfTransport.Bus,
              Library.MeansOfTransport.River,
              Library.MeansOfTransport.RiverTours,
              Library.MeansOfTransport.Tram,
              Library.MeansOfTransport.CableCar
             ),
             placeHolderText = "Enter a postcode, address, station, stop or pier"
            });
          }
        </div>
        @{ 
          Html.RenderAction("EmbeddedMap", "Maps", new
          {
            layers = "cycle-hire,station-stop",
            layersVisibleAtLoad = "cycle-hire",
            centreLat = "",
            centreLng = "",
            zoom = "13",
            showSatelliteMap = "0",
            showUserLocation = "1",
            kmlUrl = "",
            zoomThresholds = new EmbeddedMapViewModel.EmbeddedMapZoomThresholds() { DisplayBusStopsBy = 21, DisplayLargeBusStopsBy = 21 }
          });
        }      
      
  
